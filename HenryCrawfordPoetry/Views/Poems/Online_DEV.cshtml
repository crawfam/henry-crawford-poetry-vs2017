    <!-- Cube Portfolio -->
    <link rel="stylesheet" href="~/Content/assets/cubeportfolio/css/cubeportfolio.min.css">


    <!-- ANIMATE -->
    <link rel="stylesheet" href="~/Content/assets/animate/animate.min.css" type="text/css">

}


<div class="container">

    <!-- SORT LIST BAR -->
    <div class="sort-list-bar sct-color-1 style-2 text-center">
        <div class="container">
            <div id="filters-container" class="cbp-l-filters-button">
