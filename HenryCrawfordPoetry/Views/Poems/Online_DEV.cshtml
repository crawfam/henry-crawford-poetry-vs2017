@{
    ViewBag.Title = "TestOnline";
}

@section AdditionalMetatags
{
    <meta property="og:url" content="http://henrycrawfordpoetry.com/online" />
    <meta property="og:type" content="website" />
    <meta property="og:title" content="Black Hole Ekphrastic " />
    <meta property="og:description" content="The Ekphratic Review" />
