    ViewBag.Title = "Events";
}

@section AdditionalMetatags
{
    <meta property="og:url" content="http://henrycrawfordpoetry.com/Events" />
    <meta property="og:type" content="Poetry Event" />
    <meta property="og:title" content="Joaquin Miller Poetry Series" />
    <meta property="og:description" content="Readings in the Rock Creek Park" />
    <meta property="og:image" content="http://henrycrawfordpoetry.com/Images/Events/JoaquinMillerCabin.jpg" />
