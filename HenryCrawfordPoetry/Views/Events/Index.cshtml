@{
    ViewBag.Title = "Events";
}

@section AdditionalMetatags
{
    <meta property="og:url" content="http://henrycrawfordpoetry.com/Events" />
    <meta property="og:type" content="Poetry Event" />
    <meta property="og:title" content="Workshop: May 18, 2019 at 4 p.m" />
    <meta property="og:description" content="Gathersburg Book Festival" />
    <meta property="og:image" content="http://henrycrawfordpoetry.com/Images/Website/writing_poetry_for_electronic_media.jpg" />
}


<section class="sct-color-1 slice">
    <div class="container">


        <div id="gaithersburg_book_festival" class="section-title-wrapper style-1 v1">

            <h3 class="section-title center">
                <span>Gaithersburg Book Festival Workshop</span>
                <span class="section-subtitle"><small>Saturday, May 18, 2019 at 4 p.m.</small></span>
            </h3>

            <div class="row">

                <div class="col-md-9">

                    <div class="block block-image v3 mb-40">
                        <div class="block-image">
                            <img src="~/Images/Poems/henry_crawford_game_on.jpg" />
                        </div>
                        <div class="block-content">
                            <h3 class="block-title">
                                Writing Poetry for Electronic Media
                            </h3>
                            <h3>Henry Crawford on Experimental Poetry and Electronic Media</h3>
                            <p>
                                What does poetry look and sound like in the digital age? I'll be demonstrating some of the techniques for publishing in the emerging electronic press and writing multi-media poetry for online publication.
                            </p>
                            <div class="mt-15">
                                <span class="label bg-red"><a target="_blank" href="http://www.gaithersburgbookfestival.org/">Gaithersburg Book Festival</a></span>
                            </div>
                            <br />
                            <p>
                                <b>Location</b>: Gaithersburg City Hall<br />
                                31 South Summit Avenue,<br />Gaithersburg, MD 20877
                                <div class="mt-15">
                                    <span class="label bg-red"><a target="_blank" href="http://www.gaithersburgbookfestival.org/about/directions-and-parking/">Directions</a></span>
                                </div>
                            </p>
                        </div>
                    </div>

                    <span class="clearfix mt-20"></span>

                </div>

                <div class="col-md-3">
                    <!-- SIDEBAR -->



                    <div class="buttons-example mb-20">
                        @Html.ActionLink("Workshop Slides", "DownloadWorkshopSlides", "Events", null, new { @class = "btn btn-blue btn-icon btn-icon-animate fa-pencil-square-o" })
                    </div>


                </div> <!-- end sidebar -->

            </div> <!--end row -->

        </div> <!-- end section -->







    </div> <!-- end container -->

</section>



